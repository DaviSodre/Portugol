
    escreva "Digite a sua primeira nota:"
    Declarar Inteiro (M)
    
    leia (M);
    escreva ("Digite a sua segunda nota: ")
    Declarar Inteiro (M2)
    
    leia (M2);
    Se (M + M2 >= 10)
        Declarar Aprovado
        
        escreva ("Você foi aprovado!!")
    Senão
        Declarar Reprovado
        
        escreva ("Você foi reprovado, estude mais.")
    Fim
