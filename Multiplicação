programa {
  funcao inicio() {
     inteiro A, B, resultado
   
    
    escreva ("Bem vindo a multiplicação! Escreva um numero para ser calculado: ")
    leia (A)
    escreva ("Escreva outro número para ser calculado ")
    leia (B)
    
    resultado = (A * B)
    
    
    escreva ("O resultado da multiplicação é: ", resultado)
    


  }
}
